<h1>Listing series</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Anz staf</th>
    <th>Anz eps</th>
    <th>Rating</th>
    <th>Comments</th>
    <th>Episodes</th>
    <th>Anz rating</th>
    <th>Global</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @series.each do |series| %>
  <tr>
    <td><%= series.name %></td>
    <td><%= series.anz_staf %></td>
    <td><%= series.anz_eps %></td>
    <td><%= series.rating %></td>
    <td><%= series.comments %></td>
    <td><%= series.episodes %></td>
    <td><%= series.anz_rating %></td>
    <td><%= series.global %></td>
    <td><%= link_to 'Show', series %></td>
    <td><%= link_to 'Edit', edit_series_path(series) %></td>
    <td><%= link_to 'Destroy', series, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

